<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:actionBarSize">30dip</item>
        <item name="actionBarSize">50dip</item>
        <!--<item name="windowActionBarOverlay">true</item>-->
        <!--<item name="actionBarStyle">@style/GomAcitionBar</item>-->
    </style>
    <style name="title_leftmargin" >
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginTop">20dp</item>
    </style>

    <style name="title_rightmargin">
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_marginTop">20dp</item>
    </style>

<style name="text_sidemargin">
    <item name="android:layout_marginTop">25dp</item>
    <item name="android:layout_marginRight">30dp</item>
    <item name="android:layout_marginLeft">30dp</item>
    <item name="android:padding">10dp</item>
</style>

</resources>
