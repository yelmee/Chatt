<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    android:orientation="vertical">



    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/inviteListItem_imageView_profileImage"
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:src="@drawable/profile4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />


    <TextView
        android:id="@+id/inviteListItem_textView_userName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:text="유저이름"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/inviteListItem_imageView_profileImage"
        app:layout_constraintStart_toEndOf="@+id/inviteListItem_imageView_profileImage"
        app:layout_constraintTop_toTopOf="@+id/inviteListItem_imageView_profileImage" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/inviteListItem_textView_userName" />

</androidx.constraintlayout.widget.ConstraintLayout>