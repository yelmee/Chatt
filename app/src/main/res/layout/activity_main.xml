<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/fragment"
        android:layout_width="0dp"
        android:layout_height="630dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/top_border"
        android:gravity="bottom"
        android:padding="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/menubar_button_friendList"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.3"
            android:padding="7dp"
            app:srcCompat="@drawable/ic_personal_924818dc8dc601731c972c357e5806d5"
            tools:layout_editor_absoluteX="27dp"
            tools:layout_editor_absoluteY="673dp" />

        <ImageView
            android:id="@+id/menubar_button_roomList"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.3"
            android:padding="7dp"
            app:srcCompat="@drawable/ic_message"
            tools:layout_editor_absoluteX="169dp"
            tools:layout_editor_absoluteY="665dp" />

        <ImageView
            android:id="@+id/menubar_button_setting"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="0.3"
            android:padding="7dp"
            app:srcCompat="@drawable/ic_three_dots_ellipsis_svgrepo_com"
            tools:layout_editor_absoluteX="322dp"
            tools:layout_editor_absoluteY="673dp" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>